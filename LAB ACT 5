def classify_age(age):
    if age < 0:
        print("invalid age! age cannot be negative.")
    elif age <= 12:
        print("you are a child.")
    elif age <= 19:
        print("you are a teen.")
    elif age <= 64:
        print("you are an adult.")
    else:
        print("you are senior.")
        
classify_age(30)     
classify_age(19)   
classify_age(88)